{
  "name": "UpdateChecker1",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["https://www.lightnovelpub.com/browse"],
      "js": ["content.js"]
    }
  ],
  "browser_action":{
    "default_title": "Novel Updates"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "storage"]
}